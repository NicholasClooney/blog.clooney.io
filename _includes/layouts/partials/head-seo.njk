<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
{% set pageTitle = rawTitle %}
{% if environment == 'development' %}
	{% set pageTitle = "👩‍💻 DEV - " ~ pageTitle %}
{% endif %}
<title>{{ pageTitle }}</title>
{% if pageDescription %}
	<meta name="description" content="{{ pageDescription }}">
{% endif %}
<link rel="alternate" href="/feed.xml" type="application/rss+xml" title="{{ metadata.title }}">
{% if canonicalUrl %}
	<link rel="canonical" href="{{ canonicalUrl }}">
{% endif %}
<meta property="og:title" content="{{ rawTitle }}">
<meta property="og:type" content="{{ ogType }}">
<meta property="og:site_name" content="{{ siteTitle }}">
{% if canonicalUrl %}
	<meta property="og:url" content="{{ canonicalUrl }}">
{% endif %}
{% if pageDescription %}
	<meta property="og:description" content="{{ pageDescription }}">
{% endif %}
{% if ogImageUrl %}
	<meta property="og:image" content="{{ ogImageUrl }}">
	<meta property="og:image:width" content="1200">
	<meta property="og:image:height" content="630">
	{% if pageDescription %}
		<meta property="og:image:alt" content="{{ pageDescription }}">
	{% endif %}
{% endif %}
<meta name="twitter:card" content="{{ twitterCard }}">
<meta name="twitter:title" content="{{ rawTitle }}">
{% if pageDescription %}
	<meta name="twitter:description" content="{{ pageDescription }}">
{% endif %}
{% if ogImageUrl %}
	<meta name="twitter:image" content="{{ ogImageUrl }}">
{% endif %}

{#- Uncomment this if you’d like folks to know that you used Eleventy to build your site!  #}
{#- <meta name="generator" content="{{ eleventy.generator }}"> #}

<meta name="fediverse:creator" content="@NicholasClooney@mastodon.social">
